{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "extensions": {
    "durableTask": {
      "hubName": "%MyTaskHubName%",
      "storageProvider": {
        "connectionStringName":"AzureWebJobsStorageTaskHub"
      },
      "notifications": {
        "eventGrid": {
          "topicEndpoint": "https://my-training1-topic.japaneast-1.eventgrid.azure.net/api/events",
          "keySettingName": "MyEventGridKey"
        }
      }
    },
    "cosmosDB": {
      "connectionMode": "Gateway",
      "protocol": "Https",
      "leaseOptions": {
        "leasePrefix": "prefix1"
      }
    }
  }
}